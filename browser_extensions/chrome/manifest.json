{
    "manifest_version": 2,
    "name": "SDU BB Vod downloader",
    "version": "1.2",
    "description": "Helper for downloading blackboard videos at SDU.",
    "browser_action": {
        "default_title":"Download",
        "default_popup":"pop.html"
    },
    "icons": { "16": "icon.png",
      "48": "icon.png",
     "128": "icon.png" },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
      "webRequest", "<all_urls>"
    ]
  }